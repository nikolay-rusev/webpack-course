{
    "id": "header-widget",
    "cached": true,
    "static": true,
    "components": [
        {
            "type": "container",
            "bindTo": "header-widget",
            "children": [
                {
                    "type": "logo",
                    "bindTo": "{parent}.logo",
                    "extraClasses": "tq-logo"
                },
                {
                    "type": "container",
                    "bindTo": "{parent}",
                    "extraClasses": "reg-nav-wrap",
                    "children": [
                        {
                            "type": "dropdown",
                            "bindTo": "{parent}.languageSelect",
                            "skipLabel": true,
                            "extraClasses": "reg-language"
                        },
                        {
                            "type": "container",
                            "bindTo": "{parent}.navigation",
                            "extraClasses": "reg-menu-wrap tq-right",
                            "children": [
                                {
                                    "type": "menu",
                                    "bindTo": "{parent}.navMenu",
                                    "extraClasses": "reg-nav-menu"
                                },
                                {
                                    "type": "button",
                                    "bindTo": "{parent}.btn1",
                                    "extraClasses": "reg-secondary-ghost tq-offset-right"
                                },
                                {
                                    "type": "button",
                                    "bindTo": "{parent}.btn2",
                                    "color": "primary"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
